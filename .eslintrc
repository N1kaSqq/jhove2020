{
  "extends": ["airbnb", "airbnb-base"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "object-curly-newline": 0,
    "arrow-parens": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
